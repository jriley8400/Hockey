<h1>Editing Fantasy #<%= @fantasy.id %></h1>

<form action="/fantasies/<%= @fantasy.id %>/update">
  <div>
    <label for="name">name</label><br />
    <input id="name" name="name" value="<%= @fantasy.name  %>" type="text" />
  </div>
  <div>
    <label for="user">user</label><br />
    <input id="user_id" name="user_id" value="<%= @fantasy.user_id  %>" type="text" />
  </div>
  <div>
    <input type="submit">
  </div>
</form>


