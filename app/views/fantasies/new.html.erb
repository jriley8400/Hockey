<h1>New Fantasy Team</h1>

<form action="/create_fantasy">
  <div>
    <label for="name">name</label><br />
    <input id="name" name="name" type="text" />
  </div>
     <div>
    <label for="user_id">User</label><br />
    <select name="user_id">
      <% @users.each do |user| %>
      <option value="<%= user.id %>"><%= user.name %></option>
      <% end %>
    </select>
 </div>
  <div>
    <input type="submit">
  </div>
</form>

