<h1>Fplayers</h1>

<p><a href="/new_fplayer">Add a New Fplayer</a></p>

<table>
  <thead>
    <tr>
      <th>Player</th>
      <th>Fantasy</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
  <% @fplayers.each do |fplayer| %>
    <tr>
      <td><%= fplayer.player.name %></td>
      <td><%= fplayer.fantasy.name %></td>
          <td><a href="/fplayers/<%= fplayer.id %>">Show</a>
        </td>
      <td><a href="/fplayers/<%= fplayer.id %>/edit">Edit</a></td>
      <td><a href="/fplayers/<%= fplayer.id %>/destroy">Destroy</a></td>
    </tr>
  <% end %>
  </tbody>
</table>

